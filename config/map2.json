{
	"library": "Leaflet",
	"interactions": {
		"retrieve": {
			"logging": true,
			"interface": {
				"popup": false
			}
		},
		"pan": {
			"logging": true,
			"interface": {}
		},
		"zoom": {
			"logging": true,
			"interface": {}
		},
		"overlay": {
			"logging": true,
			"dataLayers": [
				"Population Density"
			]
		},
		"underlay": {
			"logging": true
		},
		"search": {
			"attributes": ["name", "density"],
			"logging": true
		},
		"filter": {
			"dataLayers": [
				"Population Density"
			],
			"tool": "slider",
			"logging": true
		},
		"reexpress": {
			"logging": true
		},
		"resymbolize": {
			"logging": true
		}
	},
	"mapOptions": {
		"center": [45,-90],
		"zoom": 7,
		"minZoom": 3,
		"maxZoom": 13
	},
	"baseLayers": [{
		"name": "Stamen Toner Lite",
		"source": "http://{s}.tile.stamen.com/toner-lite/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Map by <a href='http://stamen.com'>Stamen</a>, data by <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap contributors</a>"
			}
		},
		{
		"name": "Stamen Toner",
		"source": "http://{s}.tile.stamen.com/toner/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Map by <a href='http://stamen.com'>Stamen</a>, data by <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap contributors</a>"
			}
	}],
	"dataLayers": [{
		"name": "Population Density",
		"source": "data/wi-tracts-poly.topojson",
		"expressedAttribute": "density",
		"layerOptions": {
			"color": "black",
			"weight": 1,
			"opacity": 0.8,
			"fillOpacity": 0.8,
			"fillColor": "#FD8D3C"
		},
		"techniques": [{
			"type": "isarithmic",
			"interval": 20,
			"size": 1
		},
		{
			"type": "proportional symbol",
			"classification": "unclassed",
			"classes": [
				5,
				20
			]
		},
		{
			"type": "choropleth",
			"classification": "quantile",
			"classes": "Oranges.5"
		}]
	}]
}