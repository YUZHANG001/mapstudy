{
	"library": "Leaflet",
	"interactions": {
		"retrieve": {
			"logging": true,
			"interface": {
				"popup": false
			}
		},
		"pan": {
			"logging": true,
			"interface": {}
		},
		"zoom": {
			"logging": true,
			"interface": {}
		},
		"overlay": {
			"logging": true,
			"dataLayers": [
				"US States",
				"Cities Layer"
			]
		},
		"underlay": {
			"logging": true
		},
		"search": {
			"attributes": ["name", "Density (ppl/sq. mile)"],
			"logging": true
		},
		"filter": {
			"dataLayers": [
				"US States", 
				"Cities Layer"
			],
			"tool": "slider",
			"logging": true
		},
		"reexpress": {
			"logging": true
		},
		"resymbolize": {
			"logging": true
		}
	},
	"mapOptions": {
		"center": [39.83,-98.583],
		"zoom": 3,
		"minZoom": 3,
		"maxZoom": 13
	},
	"baseLayers": [{
		"name": "Stamen Toner Lite",
		"source": "http://{s}.tile.stamen.com/toner-lite/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Map by <a href='http://stamen.com'>Stamen</a>, data by <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap contributors</a>"
			}
		},
		{
		"name": "Stamen Toner",
		"source": "http://{s}.tile.stamen.com/toner/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Map by <a href='http://stamen.com'>Stamen</a>, data by <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap contributors</a>"
			}
	}],
	"dataLayers": [{
		"name": "Population Density",
		"source": "data/wi-tracts-point.geojson",
		"expressedAttribute": "density",
		"layerOptions": {
			"color": "black",
			"weight": 1,
			"opacity": 0.8
		},
		"techniques": [{
			"type": "isarithmic",
			"interval": 20,
			"size": 1
		}]
	}]
}