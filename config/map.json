{
	"library": "Leaflet",
	"interactions": {
		"retrieve": {
			"logging": true,
			"attributes": ["name", "density"]
		},
		"pan": {
			"logging": true
		},
		"zoom": {
			"logging": true
		},
		"overlay": {
			"logging": true,
			"dataLayers": [
				"US States Choropleth"
			]
		},
		"underlay": {
			"logging": true
		},
		"search": {
			"attributes": ["name", "density"],
			"logging": true
		}
	},
	"mapOptions": {
		"center": [39.83,-98.583],
		"zoom": 3,
		"minZoom": 3,
		"maxZoom": 13
	},
	"baseLayers": [{
		"name": "Acetate",
		"source": "http://{s}.acetate.geoiq.com/tiles/acetate/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Acetate tileset from ESRI GeoIQ"
		}
	}],
	"dataLayers": [{
		"name": "US States Choropleth",
		"source": "data/us-states.geojson",
		"postgisTable": "",
		"expressedAttribute": "density",
		"layerOptions": {
			"fillOpacity": 0.8,
			"color": "white",
			"opacity": 1
		},
		"technique": {
			"type": "choropleth",
			"classification": "quantile",
			"classes": [
				"#FEEDDE",
				"#FDBE85",
				"#FD8D3C",
				"#E6550D",
				"#A63603"
			]
		}
	}]
}