{
	"library": "Leaflet",
	"interactions": {
		"filter": {
			"logging": true,
			"byAttributes": "all"
		},
		"pan": {
			"logging": true
		},
		"zoom": {
			"logging": false
		}
	},
	"mapOptions": {
		"center": [39.83,-98.583],
		"zoom": 3,
		"minZoom": 3,
		"maxZoom": 13
	},
	"baseLayer": {
		"name": "Acetate",
		"source": "http://{s}.acetate.geoiq.com/tiles/acetate/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Acetate tileset from ESRI GeoIQ"
		}
	},
	"dataLayer": {
		"name": "US States",
		"source": "data/us-states.geojson",
		"postgisTable": "",
		"expressedAttribute": "density",
		"layerOptions": {
			"fillOpacity": 0.8,
			"color": "white",
			"opacity": 1
		},
		"technique": {
			"type": "choropleth",
			"classification": "quantile",
			"classes": [
				"#D4B9DA",
				"#C994C7",
				"#DF65B0",
				"#DD1C77",
				"#980043"
			]
		}
	}
}