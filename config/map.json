{
	"library": "Leaflet",
	"interactions": {
		"retrieve": {
			"logging": true,
			"interface": {
				"popup": false
			}
		},
		"pan": {
			"logging": true,
			"interface": {}
		},
		"zoom": {
			"logging": true,
			"interface": {}
		},
		"overlay": {
			"logging": true,
			"dataLayers": [
				"US States",
				"Cities Layer"
			]
		},
		"underlay": {
			"logging": true
		},
		"search": {
			"attributes": ["name", "Density (ppl/sq. mile)"],
			"logging": true
		},
		"filter": {
			"dataLayers": [
				"US States", 
				"Cities Layer"
			],
			"tool": "slider",
			"logging": true
		},
		"reexpress": {
			"logging": true
		},
		"resymbolize": {
			"logging": true
		}
	},
	"mapOptions": {
		"center": [39.83,-98.583],
		"zoom": 3,
		"minZoom": 3,
		"maxZoom": 13
	},
	"baseLayers": [{
		"name": "Stamen Toner Lite",
		"source": "http://{s}.tile.stamen.com/toner-lite/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Map by <a href='http://stamen.com'>Stamen</a>, data by <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap contributors</a>"
			}
		},
		{
		"name": "Stamen Toner",
		"source": "http://{s}.tile.stamen.com/toner/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Map by <a href='http://stamen.com'>Stamen</a>, data by <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap contributors</a>"
			}
	}],
	"dataLayers": [{
		"name": "Population Density",
		"source": "data/us-states.geojson",
		"expressedAttribute": "Density (ppl/sq. mile)",
		"layerOptions": {
			"color": "black",
			"weight": 1,
			"opacity": 0.8,
			"fillOpacity": 0.8,
			"fillColor": "#FD8D3C"
		},
		"techniques": [{
			"type": "heat",
			"size": 5
		},
		{
			"type": "proportional symbol",
			"classification": "unclassed",
			"classes": [
				5,
				20
			]
		},
		{
			"type": "choropleth",
			"classification": "quantile",
			"classes": "Oranges.5"
		}]
	},
	{
		"name": "Cities Layer",
		"source": "data/city-data.geojson",
		"expressedAttribute": "2011",
		"displayAttributes": [
			"name", 
			"2005",
			"2006",
			"2007",
			"2008",
			"2009",
			"2010",
			"2011"
		],
		"layerOptions": {
			"fillOpacity": 0.8,
			"color": "white",
			"fillColor": "blue",
			"opacity": 1,
			"weight": 1
		},
		"techniques": [{
			"type": "proportional symbol",
			"classification": "unclassed",
			"classes": [
				5,
				20
			]
		}]
	}]
}