{
	"library": "Leaflet",
	"interactions": {
		"retrieve": {
			"logging": true
		},
		"pan": {
			"logging": true
		},
		"zoom": {
			"logging": true
		},
		"overlay": {
			"logging": true,
			"dataLayers": [
				"US States",
				"Cities Layer"
			]
		},
		"underlay": {
			"logging": true
		},
		"search": {
			"attributes": ["name", "Density (ppl/sq. mile)"],
			"logging": true
		},
		"filter": {
			"dataLayers": [
				"US States", 
				"Cities Layer"
			],
			"tool": "slider",
			"logging": true
		},
		"reexpress": {
			"logging": true
		},
		"resymbolize": {
			"logging": true
		}
	},
	"mapOptions": {
		"center": [39.83,-98.583],
		"zoom": 3,
		"minZoom": 3,
		"maxZoom": 13
	},
	"baseLayers": [{
		"name": "OSM-BW",
		"source": "http://{s}.tile.stamen.com/toner-lite/{z}/{x}/{y}.png",
		"layerOptions": {
			"attribution": "Map by <a href='http://stamen.com'>Stamen</a>, data by <a href='http://www.openstreetmap.org/copyright'>OpenStreetMap contributors</a>"
		}
	}],
	"dataLayers": [{
		"name": "US States",
		"source": "data/us-states.geojson",
		"postgisTable": "",
		"expressedAttribute": "Density (ppl/sq. mile)",
		"displayAttributes": [
			"name", 
			"Density (ppl/sq. mile)"
		],
		"layerOptions": {
			"fillOpacity": 0.8,
			"color": "black",
			"weight": 1,
			"opacity": 1,
			"fillColor": "#FD8D3C"
		},
		"techniques": [{
			"type": "choropleth",
			"classification": "quantile",
			"classes": [
				"#FEEDDE",
				"#FDBE85",
				"#FD8D3C",
				"#E6550D",
				"#A63603"
			]
		},{
			"type": "proportional symbol",
			"classification": "unclassed",
			"classes": [
				5,
				20
			]
		}]
	},
	{
		"name": "Cities Layer",
		"source": "data/city-data.geojson",
		"expressedAttribute": "2011",
		"displayAttributes": [
			"name", 
			"2005",
			"2006",
			"2007",
			"2008",
			"2009",
			"2010",
			"2011"
		],
		"layerOptions": {
			"fillOpacity": 0.8,
			"color": "white",
			"fillColor": "blue",
			"opacity": 1,
			"weight": 1
		},
		"techniques": [{
			"type": "proportional symbol",
			"classification": "unclassed",
			"classes": [
				5,
				20
			]
		}]
	}]
}